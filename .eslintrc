{
  "parser": "babel-eslint",
  "extends": "airbnb",
  "env": {
    "browser": true,
    "mocha": true,
    "node": true
  },
  "rules": {
    "react/jsx-no-bind": 0,
    "react/prefer-stateless-function": 0,
    "comma-dangle": 0,
    "no-use-before-define": 0,
    "consistent-return": 0,
    "max-len": [2, 120],
    "no-unused-vars": 1,
    "new-cap": 0,
    "spaced-comment": 1,
    "eqeqeq": [2, "allow-null"],
    "brace-style": [2, "stroustrup"],
    "global-require": 0,
    "import/no-unresolved": 0,
    "no-console": 0,
    "no-underscore-dangle": 0
  },
  "plugins": [
    "react"
  ]
}
